	"bugs": {
		"url": "https://github.com/nitono/aeromd-react/issues"
	},
	"homepage": "https://github.com/nitono/aeromd-react#readme",
	"devDependencies": {
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3"
	}
