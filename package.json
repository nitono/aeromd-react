{
	"name": "@nitonodev/aeromd-react",
	"description": "The @nitonodev/aeromd plugin for parse markdown to react",
	"version": "1.0.0",
	"main": "lib/index.js",
	"module": "lib/index.js",
	"scripts": {
		"format": "prettier --write .",
		"start": "npm run format && nodemon src/index.ts",
